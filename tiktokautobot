def scrolling():
    pyautogui.moveTo(390, 960)
    print("Initializing")
    pyautogui.dragTo(390, 177, 10)
    time.sleep(10)
    #click()
    time.sleep(2)
    print("Initializing")
    pyautogui.moveTo(390, 960)
    pyautogui.dragTo(390, 177, 10)
    time.sleep(10)
    #click()
    time.sleep(1)
    print("Initializing")
    pyautogui.moveTo(390, 960)
    pyautogui.dragTo(390, 177, 10)
    time.sleep(10)
    #click()
def singleClick():
    pyautogui.moveTo(488, 181)
    pyautogui.mouseDown(488, 181)
    pyautogui.mouseUp(488, 181)



count = 0
while count<=198:
    pyautogui.moveTo(412, 246) #instead coordinates can be called to image
    pyautogui.dragTo(390, 174, 10)
    singleClick()
    time.sleep(10)
    pyautogui.moveTo(412, 246)
    pyautogui.dragTo(390, 174, 10)
    singleClick()
    time.sleep(10)
    print(count)
    count+=1





