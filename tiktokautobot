import pyautogui
import time

def click():
    pyautogui.moveTo(488,181)
    pyautogui.mouseDown(488,181)
    pyautogui.mouseUp(488,181)

    pyautogui.moveTo(488,244)
    pyautogui.mouseDown(488,244)
    pyautogui.mouseUp(488,244)
    time.sleep(70)
    print("First Set")


    pyautogui.moveTo(488,308)
    pyautogui.mouseDown(488,308)
    pyautogui.mouseUp(488,308)

    pyautogui.moveTo(488,375)
    pyautogui.mouseDown(488,375)
